package edu.jsp.abstraction;

public abstract class ClassA {

	static int x;
	final int y;
	public ClassA(int y) {
		this.y = y;
		System.out.println("ClassA Constructor");
	}
	//abstract void m1();
	
	void m2(){
		System.out.println("inside m2()");
	}
	
	static void m3(){
		System.out.println("inside static m3()..");
	}
	
}
