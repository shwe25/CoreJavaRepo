package edu.jsp.threads;

public class Thread1 extends Thread {
	@Override
	public void run() {
		try {
			sleep(2000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		for (int i = 0; i < 15; i++) {
			System.out.println("Thread 1");
		}
	}
}
